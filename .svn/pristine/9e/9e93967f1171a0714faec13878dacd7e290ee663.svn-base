package evnit.util.common;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

public class BaseVariable implements Serializable{
	private static final long serialVersionUID = 1L;
	
	private String m_String = null;
	private Integer m_Integer = null;
	private Double m_Double = null;
	private Boolean m_Boolean = null;
	private Byte m_Byte = null;
	private Long m_Long = null;
	private Float m_Float = null;
	private BigDecimal m_BigDecimal=null;
	private Date m_Date;
	
	public BaseVariable(){}

	public void setString(String m_String) {
		this.m_String = m_String;
	}

	public String getString() {
		return m_String;
	}

	public void setInteger(Integer m_Integer) {
		this.m_Integer = m_Integer;
	}

	public Integer getInteger() {
		return m_Integer;
	}

	public void setDouble(Double m_Double) {
		this.m_Double = m_Double;
	}

	public Double getDouble() {
		return m_Double;
	}

	public void setBoolean(Boolean m_Boolean) {
		this.m_Boolean = m_Boolean;
	}

	public Boolean getBoolean() {
		return m_Boolean;
	}

	public void setByte(Byte m_Byte) {
		this.m_Byte = m_Byte;
	}

	public Byte getByte() {
		return m_Byte;
	}

	public void setLong(Long m_Long) {
		this.m_Long = m_Long;
	}

	public Long getLong() {
		return m_Long;
	}

	public void setFloat(Float m_Float) {
		this.m_Float = m_Float;
	}

	public Float getFloat() {
		return m_Float;
	}
	
	public void setBigDecimal(BigDecimal val) {
		this.m_BigDecimal = val;
	}

	public BigDecimal getBigDecimal() {
		return m_BigDecimal;
	}

	public void setDate(Date m_Date) {
		this.m_Date = m_Date;
	}

	public Date getDate() {
		return m_Date;
	}	
}
