       }

       function replaceFilter()
       {
           var oFilter;
           oFilter=document.getElementById('formDlg:dtFunction:funcid_filter');
           if (oFilter)
           {
               oFilter.onkeyup = function () {filterByEnter(event);};
           }

       }
       init();       
    </script>
</html>